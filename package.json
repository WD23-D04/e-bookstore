{
  "name": "e-bookstore",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "sass": "sass --watch ./src/styles/main.scss:./src/styles/main.css",
    "test": "vitest --ui",
    "testcov": "vitest --coverage",
    "sc": "clear && git log --pretty=format:\"%h - %ad - %s (%an)\" -10 --date=format:\"%Y-%m-%d %H:%M:%S\"",
    "sca": "clear && git log --pretty=format:\"%h - %ad - %s (%an)\" --date=format:\"%Y-%m-%d %H:%M:%S\""
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@vitest/coverage-v8": "^0.34.2",
    "@vitest/ui": "^0.34.2",
    "sass": "^1.64.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "dependencies": {
    "json-server": "^0.17.3",
    "vitest": "^0.34.2"
  }
}